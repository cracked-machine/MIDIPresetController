{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            // https://wiki.segger.com/J-Link_Visual_Studio_Code
            "type": "cortex-debug",
            "request": "launch",
            "name": "Debug J-Link IP",
            "cwd": "${workspaceRoot}",
            "showDevDebugOutput": "raw",
            "executable": "${workspaceRoot}/build/build.elf",
			"serverpath": "/opt/JLink_Linux_V784a_x86_64/JLinkGDBServerCLExe",
            // "servertype": "external",
            // "gdbTarget": "192.168.1.192:9999",
            "servertype": "jlink",
            "ipAddress": "192.168.1.192:9999",
            "device": "STM32L011D4",
            "interface": "swd",
            "runToEntryPoint": "main",
            "svdFile": "${workspaceRoot}/STM32L0x1.svd",
            "rttConfig": {
                "enabled": true,
                "address": "auto",
                "decoders": [
                    {
                        "label": "",
                        "port": 0,
                        "type": "console"
                    }
                ]
            }
        }
    ]
}